{
    "name": "sec-backend",
    "version": "1.0.0",
    "description": "Share-E-Care backend",
    "main": "index.js",
    "author": "Ahmed Sharief R",
    "license": "MIT",
    "private": true,
    "scripts": {
        "dev": "sls offline --disableCookieValidation",
        "format": "eslint . --fix",
        "lint": "eslint .",
        "lint:updated": "pipe-git-updated --ext=js -- eslint",
        "prepare": "husky install",
        "prepare-release": "standard-version && prettier --write CHANGELOG.md",
        "prettier": "prettier --check .",
        "prettier-check": "prettier -c . --ignore-path .gitignore ",
        "prettier-check:updated": "pipe-git-updated --ext=css --ext=html --ext=js --ext=json --ext=md --ext=yaml --ext=yml --base=main -- prettier -c",
        "prettify": "prettier --write . --ignore-path .gitignore ",
        "prettify:updated": "pipe-git-updated --ext=css --ext=html --ext=js --ext=json --ext=md --ext=yaml --ext=yml -- prettier --write"
    },
    "standard-version": {
        "skip": {
            "commit": true,
            "tag": true
        },
        "types": [
            {
                "type": "feat",
                "section": "Features"
            },
            {
                "type": "fix",
                "section": "Bug Fixes"
            },
            {
                "type": "perf",
                "section": "Performance Improvements"
            },
            {
                "type": "refactor",
                "section": "Maintenance Improvements"
            }
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.21.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
        "@babel/plugin-transform-modules-commonjs": "^7.21.2",
        "@babel/plugin-transform-runtime": "^7.21.4",
        "@babel/preset-env": "^7.21.4",
        "@commitlint/cli": "^17.1.2",
        "@commitlint/config-conventional": "^17.1.0",
        "babel-jest": "^29.2.0",
        "babel-loader": "^9.1.2",
        "babel-preset-es2015": "^6.24.1",
        "eslint": "^8.24.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "git-list-updated": "^1.2.1",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "prettier": "^2.7.1",
        "serverless-domain-manager": "^7.0.2",
        "serverless-offline": "^12.0.4",
        "serverless-webpack": "^5.11.0",
        "standard-version": "^9.5.0",
        "webpack": "^5.78.0",
        "webpack-node-externals": "^3.0.0"
    },
    "dependencies": {
        "@aws-sdk/client-apigatewaymanagementapi": "^3.290.0",
        "canvas": "^2.11.0",
        "firebase-admin": "^11.5.0",
        "jsonwebtoken": "^9.0.0",
        "mysql": "^2.18.1",
        "serverless-prune-plugin": "^2.0.2",
        "sib-api-v3-sdk": "^8.5.0"
    }
}
